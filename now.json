{
	"name": "telegram-bots",
	"version": 2,
	"env": {
		"SECRET_STRING": "@tg-bots-secret-string",
		"CHAT_ID": "@tg-chat-id",
		"GIST_ID": "@tg-bots-gist"
	},
	"github": {
		"enabled": true,
		"silent": true
	},
	"builds": [
		{
			"src": "index.js",
			"use": "@now/node-server",
			"config": {
				"maxLambdaSize": "45mb"
			}
		}
	],
	"routes": [
		{
			"src": "/(.*)",
			"dest": "index.js"
		}
	]
}
